<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="400.0" minHeight="400.0" minWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipr.zezacracolici.ClientMenuController">
   <children>
      <VBox alignment="CENTER" layoutX="91.0" layoutY="85.0" spacing="10.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Client menu" />
            <Button fx:id="searchProduct" mnemonicParsing="false" onAction="#onOpenDialogSearch" text="Search Product" />
            <Button fx:id="buyProduct" mnemonicParsing="false" onAction="#onOpenDialogBuy" text="Buy Product" />
            <TableView fx:id="tvData" prefHeight="300.0" prefWidth="400.0">
              <columns>
                <TableColumn fx:id="colId" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="colNameProduct" prefWidth="75.0" text="Name Product" />
                <TableColumn fx:id="colNameFactory" prefWidth="75.0" text="Name Factory" />
                <TableColumn fx:id="colPrice" prefWidth="75.0" text="Price" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
      <Button fx:id="logOut" layoutX="30.0" layoutY="14.0" mnemonicParsing="false" onAction="#onOpenLogOut" text="Log out" />
   </children>
</AnchorPane>
